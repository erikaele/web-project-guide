{
  "name": "web-project-guides",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "build": "npm-run-all clean-build build:ts build:11ty -p minify:* build:css",
    "build:css": "postcss src/css/app.css -o css/global.css",
    "build:11ty": "eleventy",
    "build:ts": "spack",
    "clean-build": "rimraf build",
    "postbuild": "node resources/tooling/copy-files.js",
    "watch:ts": "spack && chokidar src/js/*.ts -c spack",
    "watch:11ty": "eleventy --serve",
    "start": "cross-env NODE_ENV=dev npm-run-all -p watch:*",
    "debug": "DEBUG=* eleventy"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^1.0.0",
    "@11ty/eleventy-img": "^1.1.0",
    "@11ty/eleventy-navigation": "^0.3.2",
    "@11ty/eleventy-plugin-syntaxhighlight": "^4.0.0",
    "@swc/cli": "^0.1.59",
    "@swc/core": "^1.3.25",
    "autoprefixer": "^10.4.2",
    "browserslist": "^4.21.4",
    "chokidar-cli": "^3.0.0",
    "cross-env": "^7.0.3",
    "cssnano": "^5.1.14",
    "luxon": "^2.3.1",
    "markdown-it": "^12.3.2",
    "markdown-it-anchor": "^8.4.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.20",
    "postcss-cli": "^9.1.0",
    "postcss-import": "^15.1.0",
    "postcss-preset-env": "^7.8.3",
    "rimraf": "^3.0.2",
    "terser": "^5.16.1"
  },
  "browserslist": [
    "defaults"
  ],
  "volta": {
    "node": "16.14.0"
  },
  "dependencies": {
    "open-props": "^1.5.3",
    "postcss-jit-props": "^1.0.9"
  }
}
